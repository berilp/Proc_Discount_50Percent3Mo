				   bIsFixedRecovery,
				   iSalesChargeDueDays,
				   hRate,
				   bMultiUnit,
				   hCMLChargesCalc
				   )
      SELECT cs.htenant,
                   cs.hamendment,
                   /* CONVERT(DATETIME, ( Dateadd(day, 1, @CurrentDate ) ), 101),*/
                   DATEADD(DAY, 1, EOMONTH(@CurrentDate)),
				   NULL,
                   cs.hchargecode,
                   cs.hamendmentinactive,
                   cs.itype,
				   cs.cFixedOffset,
				   cs.hEstimateCharge,
				   cs.hMiscCharge,
				   cs.hTrueUpCharge,
				   cs.iAuditedReportDue,
				   cs.iEOYMonth,
